@model IEnumerable<DGSappSem2Final.Models.Terms.Term>

@{
    ViewBag.Title = "Index";
}

<h2>Terms</h2>

<p>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TermName)
