@model IEnumerable<DGSappSem2Final.Models.Assements.StudentAssessment>

@{
    ViewBag.Title = "Index";
}

<h2>Assessments</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StaffAssessment.AssessmentName)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.StaffAssessmentName)
        </th>*@
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StaffAssessment.AssessmentName)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.StaffAssessmentName)
        </td>*@
        <td>
            @Html.ActionLink("Download", "Download", new { id=item.StudentAssessmentID })
        </td>
    </tr>
}

</table>
