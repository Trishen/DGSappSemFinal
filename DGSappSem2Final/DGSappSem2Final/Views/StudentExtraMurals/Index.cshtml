@model IEnumerable<DGSappSem2Final.Models.Murals.StudentExtraMural>

@{
    ViewBag.Title = "Index";
}

<h2>Student Extramural</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StudentName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExtraMural)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExtraMuralAgeGroup)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StudentName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExtraMural)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExtraMuralAgeGroup)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.StudentMuralId }) |
                @Html.ActionLink("Details", "Details", new { id = item.StudentMuralId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.StudentMuralId })
            </td>
        </tr>
    }

</table>
